{
    "name": "excell-mobility/integration-layer",
    "description": "Does the integration magic for all the services.",
    "type": "project",
    "homepage": "https://github.com/zendframework/zend-expressive-skeleton",
    "authors": [
        {
            "name": "Andr√© Rademacher",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.1",
        "zendframework/zend-stdlib": "^3.1",
        "zendframework/zend-expressive-fastroute": "^1.2",
        "zendframework/zend-servicemanager": "^3.1",
        "zendframework/zend-expressive-zendviewrenderer": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "squizlabs/php_codesniffer": "^2.7",
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
